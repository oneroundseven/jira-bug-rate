<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%=title%></title>
    <link rel="stylesheet" type="text/css" href="/pages/home.css" />
</head>
<body>

<header>
    <div class="title-bar">
        <div class="title-bar-left">
            <a class="title-bar-title bugs-log" href="/">BUG <strong>Rate</strong></a>
        </div>
        <div class="title-bar-right">
            <ul class="menu float-right">
                <li><input type="search" placeholder="Search"></li>
                <li><button type="button" class="button">Search</button></li>
            </ul>
        </div>
    </div>
</header>
<% if (allTask) { %>
<div class="content-wrap">
    <% allTask.forEach(function(task, index) { %>
    <h3 class="version-text"><%=task['fixVersion']%></h3>
    <div class="version-timeline">
        <div class="m-step step-5">
            <div class="step-item step-ed">
                <span class="dot"></span>
                <i class="line"></i>
                <div class="txt-box">
                    <div class="time-start">2017-12-18</div>
                    <div class="time-desc">Start Dev</div>
                </div>
            </div>
            <div class="step-item step-ed">
                <span class="dot"></span>
                <i class="line"></i>
                <div class="txt-box">
                    <div class="time-start"><% if (task['releaseTestTime']) { %><%=task['releaseTestTime']%><% } else { %>-<% } %></div>
                    <div class="time-desc">Start Testing</div>
                </div>
            </div>
            <div class="step-item step-ing">
                <span class="dot"></span>
                <i class="line"></i>
                <div class="txt-box">
                    <div class="time-start"><% if (task['releasePTime']) { %><%=task['releasePTime']%><% } else { %>-<% } %></div>
                    <div class="time-desc">Release -P</div>
                </div>
            </div>
            <div class="step-item">
                <span class="dot"></span>
                <i class="line"></i>
                <div class="txt-box">
                    <div class="time-start">-</div>
                    <div class="time-desc">Customer Accepting</div>
                </div>
            </div>
            <div class="step-item">
                <span class="dot"></span>
                <div class="txt-box">
                    <div class="time-start"><% if (task['releaseTime']) { %><%=task['releaseTime']%><% } else { %>-<% } %></div>
                    <div class="time-desc">Release Product</div>
                </div>
            </div>
        </div>
    </div>
    <div class="version-bugs">
        <table>
            <thead>
            <tr>
                <th width="100">FE Developer</th>
                <th>Dev Time</th>
                <th>Test Bugs</th>
                <th>Release P Bugs</th>
                <th>Req Update</th>
                <th width="400">Related Tasks</th>
            </tr>
            </thead>
            <tbody>
                <% var testBugs = 0; var pBugs = 0; var reqUpdate = 0; var workTime = 0; %>
                <% task['users'].forEach(function(user, index) { %>
                <tr>
                    <td><%=user['assignee']%></td>
                    <td>
                        <% if (user['devTime']) { %>
                        <%=user['devTime']%> h<br>
                        <%=(user['devTime']/7.5).toFixed(2)%> days
                        <% workTime += user['devTime'] %>
                        <% } else { %>-<% } %>
                    </td>
                    <td>
                        <% if (user['tbugs']) { %>
                            <%=user['tbugs']%><br>
                            <% if (user['testBugsRate']) { %><%=user['testBugsRate']%>/day<% } %>
                            <% testBugs += user['tbugs'] %>
                        <% } else { %>-<% } %>
                    </td>
                    <td>
                        <% if (user['pbugs']) { %>
                            <%=user['pbugs']%><br>
                            <% if (user['PBugsRate']) { %><%=user['PBugsRate']%>/day<% } %>
                            <% pBugs += user['pbugs'] %>
                        <% } else { %>-<% } %>
                    </td>
                    <td>
                        <% if (user['reqUpdated']) { %>
                            <%=user['reqUpdated']%>
                            <% reqUpdate += user['reqUpdated'] %>
                        <% } else { %>-<% } %>
                    </td>
                    <td>
                        <ul>
                        <% user['tasks'].forEach(function(subTask, index) { %>
                            <li><a href="<%=subTask['link']%>" target="_blank"><%=subTask['taskName']%></a></li>
                        <% }) %>
                        </ul>
                    </td>
                </tr>
                <% }) %>
                <tr>
                    <td>总计</td>
                    <td><%=workTime%> h<br><%=(workTime/7.5).toFixed(2)%> days</td>
                    <td><%=testBugs%></td>
                    <td><%=pBugs%></td>
                    <td><%=reqUpdate%></td>
                    <td>-</td>
                </tr>
            </tbody>
        </table>
    </div>
    <% }) %>
</div>
<% } %>

